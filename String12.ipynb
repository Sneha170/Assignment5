    "def to_uppercase(str):\n",
    "    num_upper = 0\n",
    "    for letter in str[:4]: \n",
    "        if letter.upper() == letter:\n",
    "            num_upper += 1\n",
    "    if num_upper >= 2:\n",
    "        return str.upper()\n",
    "    return str\n",
    "str=input(\"Enter a String :\")\n",
    "print(\"Output :\",to_uppercase(str))"
   ]
