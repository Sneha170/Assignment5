    "    maketrans=str.maketrans\n",
    "    res=str.translate(maketrans(',.','.,',' '))\n",
    "    return res.replace(',',',')\n",
    "str=input(\"Enter a String :\")\n",
    "print(swap_comma_dot(str))"
