    "def Add(str):\n",
    "    length=len(str)\n",
    "    if length>2:\n",
    "        if str[-3:]=='ing':\n",
    "            str=str+'ly'\n",
    "        else:\n",
    "            str=str+'ing'\n",
    "    return str\n",
    "str1=input(\"Enter a String :\")\n",
    "print(\"output :\",Add(str1))\n",
    "str2=input(\"Enter a String :\")\n",
    "print(\"output :\",Add(str2))"
