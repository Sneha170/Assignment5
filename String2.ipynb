   "source": [
    "def char_frequency(Str):\n",
    "    dict={}\n",
    "    for n in Str:\n",
    "        keys=dict.keys()\n",
    "        if n in keys:\n",
    "            dict[n]+=1\n",
    "        else:\n",
    "            dict[n]=1\n",
    "    return dict\n",
    "Str=input(\"Enter a String : \")\n",
    "print(char_frequency(Str))"
   ]
