    "def duplicate(str):\n",
    "    if len(str)<2:\n",
    "        return str\n",
    "    if str[0]!=str[1]:\n",
    "        return str[0]+duplicate(str[1:])\n",
    "    return duplicate(str[1:])\n",
    "str=input(\"Enter a string :\")\n",
    "print(\"output \",duplicate(str))"
